<?xml version="1.0" encoding="UTF-8"?>
<!-- Palladio FSM - Scheme for Test1 -->
<!-- Pay attention that attributeFormDefault MUST be qualified! -->
<xs:schema elementFormDefault="qualified" attributeFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" 
targetNamespace="http://Test1-Namespace.com/attribute" xmlns="http://Test1-Namespace.com/attribute">

	<xs:element name="attribute">
		<xs:complexType>
			<xs:attribute name="GUID" type="xs:string" use="required"/>
			<xs:attribute name="displayName" type="xs:string" use="required"/>
			<xs:attribute name="description" type="xs:string" use="required"/>
			<xs:attribute name="valueType" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	
</xs:schema>
