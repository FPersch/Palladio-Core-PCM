<%=Level(5,False)%><% For Each col in PKs %>.Parameters.Add("@<%= col.name %>", crit.<%= col.name %><%=GetSProcParmExt(col)%>)   '<%=GetVBVariableType(col)%><%=Level(5,False)%><% Next %>	
