<%=Level(3,False)%>'Throw New System.Security.SecurityException("Sorry, but you have not been set up to Delete a <%If IsCollectionCode Then %><%= GetCustomCollectionName %><%Else%><%= GetCustomClassName() %><% End If%>")
<%=Level(2,False)%>'End If	

<%=Level(2,False)%>DataPortal.Delete(New Criteria(False,<% For Each col in PKs %><%= col.name %><% IF PKs.Item(PKOrd).name <> col.name Then %>, <% END IF%><% Next %>) )
<%=Level(1,False)%>End Sub

<%=Level(1,False)%>Public Shared Function Get<%If IsCollectionCode Then %><%= GetCustomCollectionName %><%Else%><%= GetCustomClassName() %><% End If%>(<% For Each col in PKs %>ByVal <%= col.name %> As <%= GetVBVariableType(col) %><% IF PKs.Item(PKOrd).name <> col.name Then %>, <% END IF%><% Next %>) As <%If IsCollectionCode Then %><%= GetCustomCollectionName %><%Else%><%= GetCustomClassName() %><% End If%>
<%=Level(2,False)%>'Load an Existing Object from DB
<%=Level(2,False)%>Return CType(DataPortal.Fetch(New Criteria(False, <% For Each col in PKs %><%= col.name %><% IF PKs.Item(PKOrd).name <> col.name Then %>, <% END IF%><% Next %>)), <%If IsCollectionCode Then %><%= GetCustomCollectionName %><%Else%><%= GetCustomClassName() %><% End If%>)
<%=Level(1,False)%>End Function
	
<%=Level(1,False)%>Friend Shared Function Get<%If IsCollectionCode Then %><%= GetCustomCollectionName %><%Else%><%= GetCustomClassName() %><% End If%>Child(<% For Each col in PKs %>ByVal <%= col.name %> As <%= GetVBVariableType(col) %><% IF PKs.Item(PKOrd).name <> col.name Then %>, <% END IF%><% Next %>) As <%If IsCollectionCode Then %><%= GetCustomCollectionName %><%Else%><%= GetCustomClassName() %><% End If%>
<%=Level(2,False)%>'Load an Existing Object from DB
<%=Level(2,False)%>Return CType(DataPortal.Fetch(New Criteria(True, <% For Each col in PKs %><%= col.name %><% IF PKs.Item(PKOrd).name <> col.name Then %>, <% END IF%><% Next %>)), <%If IsCollectionCode Then %><%= GetCustomCollectionName %><%Else%><%= GetCustomClassName() %><% End If%>)
<%=Level(1,False)%>End Function
		
