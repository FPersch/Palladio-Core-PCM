<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="R Visualisation Action Set"
            visible="true"
            id="de.uka.ipd.sdq.codegen.rvisualisation.actionSet">
         <action
               label="Show &amp;R Visualisation"
               icon="icons/sample.gif"
               class="de.uka.ipd.sdq.codegen.rvisualisation.actions.OpenRReportAction"
               tooltip="Hello, Eclipse world"
               menubarPath="simuBenchMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="de.uka.ipd.sdq.codegen.rvisualisation.actions.SampleAction">
         </action>
      </actionSet>
   </extension>

      <extension
            point="org.eclipse.ui.editors">
         <editor
               class="de.uka.ipd.sdq.codegen.rvisualization.views.RReportView"
               icon="icons/sample.gif"
               id="de.uka.ipd.sdq.codegen.rvisualization.views.ReportView"
               name="R Report">
         </editor>
      </extension>


</plugin>
