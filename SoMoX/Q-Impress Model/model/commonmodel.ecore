  <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
    <details key="documentation" value="The package Core_model describes Common Service Architecture Metamodel used in scope of Q-ImPreSS project. This package contains more specialized sub-packages, like Allocation-, Annotation-, Behavior-, Deployment-, StaticStructure- and UsageModel-sub-packages."/>
  </eAnnotations>
  <eSubpackages name="ResourceEnvironment" nsURI="http:///CoreModel/ResourceEnvironment.ecore"
      nsPrefix="CoreModel.ResourceEnvironment">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="The ResourceEnvironment package specifies Hardware- and Software-Resources and is used as Allocation Target. Supports Modelling of CPUs, Disks, Network Links, and Main Memory."/>
    </eAnnotations>
    <eClassifiers xsi:type="ecore:EClass" name="Node">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The Node entity defines deployment node with association to provided resources and building resource enviroment."/>
      </eAnnotations>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="Allocation" nsURI="http:///CoreModel/Allocation.ecore" nsPrefix="CoreModel.Allocation">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="The package Allocation encapsulates allocation concerns of components. Deployed component on a deployment Node is Service and can be called by Clients (potentially over the Network).&#xD;&#xA;"/>
    </eAnnotations>
    <eClassifiers xsi:type="ecore:EClass" name="Service" eSuperTypes="#//StaticStructure/CompositeStructure">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Under the Element Service we understand service as (implicitly) deployed component, therefore Service Metaclass is part of Allocation Package. "/>
      </eAnnotations>
      <eOperations name="isBlackBox" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="self.subcomponents->size() = 0"/>
        </eAnnotations>
        <eParameters name="diagnostics" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDiagnosticChain">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The chain of diagnostics to which problems are to be appended."/>
          </eAnnotations>
        </eParameters>
        <eParameters name="context">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The cache of context-specific information."/>
          </eAnnotations>
          <eGenericType eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EMap">
            <eTypeArguments eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EJavaObject"/>
            <eTypeArguments eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EJavaObject"/>
          </eGenericType>
        </eParameters>
      </eOperations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isBlackBox" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
          volatile="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Each service could be BlackBox (derived attribute isBlackBox is true if subcomponents = 0) or composition that is readable and has no BlackBox property."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="node" ordered="false"
          lowerBound="1" upperBound="-1" eType="#//ResourceEnvironment/Node">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The node property of class Service determines the hardware node on which the service is run, i.e., this is the node on which the component referenced by the component property is allocated. The allocated component uses the hardware parts (e.g., CPU, disk, or memory) of this node."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="component" ordered="false"
          lowerBound="1" eType="#//StaticStructure/ComponentType">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Each of introduced Services has a one component type associated. "/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="Behavior" nsURI="http:///CoreModel/Behavior.ecore" nsPrefix="CoreModel.Behavior">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="The package Behavior specifies behavior models of Services. Finding a Common Behaviour Model for different types of analyses is hard if not impossible to achive, because all of the models have its certain advatages. Therefore, Q-ImPrESS Services support an extensible set of Behaviour Models, which can be mapped on the Metamodel encapsulated in this package.&#xD;&#xA;"/>
    </eAnnotations>
    <eClassifiers xsi:type="ecore:EClass" name="Behavior" abstract="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The class Behavior is the root class definning Behavior model associated with a service interface. "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="operation" ordered="false"
          eType="#//StaticStructure/Operation">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The operation property allows externall call of operations from service behavior model."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="Annotation" nsURI="http:///CoreModel/Annotation.ecore" nsPrefix="CoreModel.Annotation">
