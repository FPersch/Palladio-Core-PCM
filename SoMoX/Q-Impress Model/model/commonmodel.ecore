      <details key="documentation" value="The package StaticStructure defines Services, their Interfaces, and their Bindings. Is the main package of core metamodel defining static structure of applications.&#xD;&#xA;"/>
    </eAnnotations>
    <eClassifiers xsi:type="ecore:EClass" name="CompositeStructure" abstract="true"
        eSuperTypes="#//StaticStructure/ComponentType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
