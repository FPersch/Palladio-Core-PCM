<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.debug.core.launchConfigurationTypes">
	    <launchConfigurationType
           delegate="de.uka.ipd.sdq.codegen.simucontroller.runconfig.SimuLaunchConfigurationDelegate"
           id="de.uka.ipd.sdq.simucontroller.SimuLaunching"
           modes="run, debug"
           name="SimuBench"
           public="true">        
	    </launchConfigurationType>
	</extension>
	<extension point="org.eclipse.debug.ui.launchConfigurationTypeImages">
	    <launchConfigurationTypeImage
	        icon="icons/app_simucim.gif"
	        configTypeID="de.uka.ipd.sdq.simucontroller.SimuLaunching"
	        id="de.uka.ipd.sdq.simucontroller.launchConfigurationTypeImage">
	    </launchConfigurationTypeImage>
	</extension>
	<extension point="org.eclipse.debug.ui.launchConfigurationTabGroups">
	    <launchConfigurationTabGroup
	        type="de.uka.ipd.sdq.simucontroller.SimuLaunching"
	        class="de.uka.ipd.sdq.codegen.simucontroller.runconfig.SimuTabGroup"
	        id="de.uka.ipd.sdq.simucontroller.launchConfigurationTabGroup">
	    </launchConfigurationTabGroup>
	</extension>
	<extension point="org.eclipse.debug.core.launchConfigurationTypes">
	    <launchConfigurationType
           delegate="de.uka.ipd.sdq.codegen.simucontroller.runconfig.SimuLaunchConfigurationDelegate"
           id="de.uka.ipd.sdq.simucontroller.SimuLaunching.Reliability"
           modes="run, debug"
           name="SimuBench Reliability"
           public="true">        
	    </launchConfigurationType>
	</extension>
	<extension point="org.eclipse.debug.ui.launchConfigurationTypeImages">
	    <launchConfigurationTypeImage
	        icon="icons/app_simucim.gif"
	        configTypeID="de.uka.ipd.sdq.simucontroller.SimuLaunching.Reliability"
	        id="de.uka.ipd.sdq.simucontroller.launchConfigurationTypeImage.Reliability">
	    </launchConfigurationTypeImage>
	</extension>
	<extension point="org.eclipse.debug.ui.launchConfigurationTabGroups">
	    <launchConfigurationTabGroup
	        type="de.uka.ipd.sdq.simucontroller.SimuLaunching.Reliability"
	        class="de.uka.ipd.sdq.codegen.simucontroller.runconfig.SimuTabGroup"
	        id="de.uka.ipd.sdq.simucontroller.launchConfigurationTabGroup.Reliability">
	    </launchConfigurationTabGroup>
	</extension>
 <extension
       point="org.eclipse.ui.views">
    <view
          allowMultiple="false"
          class="de.uka.ipd.sdq.codegen.simucontroller.gui.DockStatusViewPart"
          icon="icons/screen_blue_small.gif"
          id="de.uka.ipd.sdq.codegen.simucontroller.gui.DockStatusViewPart"
          name="Simulation Dock Status">
    </view>
 </extension>

	
</plugin>
