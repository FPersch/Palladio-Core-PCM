<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="cost"
    nsURI="http://sdq.ipd.uka.de/CostModel/1.0" nsPrefix="cost">
  <eClassifiers xsi:type="ecore:EClass" name="ComponentCost" eSuperTypes="#//Cost">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="For components, the componentOperatingCost and componentInitialCost are directly specified and used to derive the operatingCost and the initialCost 1:1. "/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="componentOperatingCost"
        ordered="false" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="componentInitialCost" ordered="false"
        lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="repositoryComponent" ordered="false"
        lowerBound="1" eType="ecore:EClass ../../de.uka.ipd.sdq.pcm/model/pcm.ecore#//repository/RepositoryComponent"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Cost" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="annotatedElement" ordered="false"
        lowerBound="1" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EObject"
        changeable="false" volatile="true" derived="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operatingCost" ordered="false"
        lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
        changeable="false" volatile="true" transient="true" derived="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The value of this attributes has to be derived in subclasses. "/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="initialCost" ordered="false"
        lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
        changeable="false" volatile="true" transient="true" derived="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FixedLinkingResourceCost" eSuperTypes="#//Cost">
    <eStructuralFeatures xsi:type="ecore:EReference" name="linkingresource" ordered="false"
        lowerBound="1" eType="ecore:EClass ../../de.uka.ipd.sdq.pcm/model/pcm.ecore#//resourceenvironment/LinkingResource"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FixedProcessingResourceCost" eSuperTypes="#//Cost">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="need additional repository of available resource &quot;subtypes&quot;. "/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="processingresourcespecification"
        ordered="false" lowerBound="1" eType="ecore:EClass ../../de.uka.ipd.sdq.pcm/model/pcm.ecore#//resourceenvironment/ProcessingResourceSpecification"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VariableCost" abstract="true" eSuperTypes="#//Cost">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="fixedInitialCost" ordered="false"
        lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="fixedOperatingCost" ordered="false"
        lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VariableLinkingResourceCost" eSuperTypes="#//VariableCost">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="see VariableProcessingResourceCost, this one works analogously. "/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="bandwithInitialFactor"
        ordered="false" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="bandwidthOperatingFactor"
        ordered="false" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="linkingresource" ordered="false"
        lowerBound="1" eType="ecore:EClass ../../de.uka.ipd.sdq.pcm/model/pcm.ecore#//resourceenvironment/LinkingResource"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VariableProcessingResourceCost" eSuperTypes="#//VariableCost">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="VariableProcessingResourceCost are attached to ProcessingResourceSpecification. They derive the operating cost and inital cost based on the ptocessingRate of the resource, multiplied by the processing rate (operating or initial) factor. Additionally, the fixed initial costs as specified in VariableCost are added to the respective cost."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="processingRateInitialFactor"
        ordered="false" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="processingRateOperatingFactor"
        ordered="false" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="processingresourcespecification"
        ordered="false" lowerBound="1" eType="ecore:EClass ../../de.uka.ipd.sdq.pcm/model/pcm.ecore#//resourceenvironment/ProcessingResourceSpecification"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CostRepository">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="The cost repository is the root element of the cost model. It contains all costs for a PCM instance. &#xD;&#xA;&#xD;&#xA;At first, we assume that all costs in this repository are actually costs in the system. Later, if we have FixedProcessingResourceCost for certain types of, for example, CPUs, we have to check the model first which annotated elements are actually used. Same with alternative components. "/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="cost" ordered="false" upperBound="-1"
        eType="#//Cost" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
