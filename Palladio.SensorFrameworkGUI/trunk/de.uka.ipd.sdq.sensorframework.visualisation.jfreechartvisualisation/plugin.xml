<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
      <extension
            point="org.eclipse.ui.editors">
         <editor
               class="de.uka.ipd.sdq.sensorframework.visualisation.jfreechartvisualisation.editor.JFreeChartHistogramReport"
               icon="icons/new_chart.gif"
               id="de.uka.ipd.sdq.simucomframework.visualisation.JFreeChartHistogramReport"
               name="JFreeChart Histogram Viewer">
         </editor>
         <editor
               class="de.uka.ipd.sdq.sensorframework.visualisation.jfreechartvisualisation.editor.JFreeChartPieReport"
               icon="icons/new_chart.gif"
               id="de.uka.ipd.sdq.simucomframework.visualisation.JFreeChartPieReport"
               name="JFreeChart Pie Viewer">
         </editor>
      </extension>
      <extension
            point="de.uka.ipd.sdq.sensorframework.visualisation">
         <Visualisation
               acceptsData="de.uka.ipd.sdq.codegen.simudatavisualisation.datatypes.Pie"
               acceptsMultiple="false"
               class="de.uka.ipd.sdq.codegen.simudatavisualization.views.JFreeChartPieReport"
               displayName="JFreeChart Pie Chart"
               editorID="de.uka.ipd.sdq.simucomframework.visualisation.JFreeChartPieReport">
         </Visualisation>
      </extension>
      <extension
            point="de.uka.ipd.sdq.sensorframework.visualisation">
         <Visualisation
               acceptsData="de.uka.ipd.sdq.codegen.simudatavisualisation.datatypes.Histogram"
               acceptsMultiple="true"
               class="de.uka.ipd.sdq.sensorframework.visualisation.jfreechartvisualisation.editor.JFreeChartPieReport"
               displayName="JFreeChart Histogram"
               editorID="de.uka.ipd.sdq.simucomframework.visualisation.JFreeChartHistogramReport">
         </Visualisation>
      </extension>

</plugin>
