<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
      <extension
            point="de.uka.ipd.sdq.sensorframework.visualisation">
         <Visualisation
               acceptsData="de.uka.ipd.sdq.sensorframework.entities.TimeSpanSensor"
               acceptsMultiple="true"
               class="de.uka.ipd.sdq.codegen.rvisualisation.views.RTimeSeriesReportView"
               displayName="R TimeSensor Report"
               editorID="de.uka.ipd.sdq.codegen.rvisualization.views.ReportView">
         </Visualisation>
      </extension>
      <extension
            point="org.eclipse.ui.editors">
         <editor
               class="de.uka.ipd.sdq.codegen.rvisualisation.views.RTimeSeriesReportView"
               icon="icons/sample.gif"
               id="de.uka.ipd.sdq.codegen.rvisualization.views.ReportView"
               name="R Report">
         </editor>
      </extension>

      <extension
            point="de.uka.ipd.sdq.sensorframework.visualisation">
         <Visualisation
               acceptsData="de.uka.ipd.sdq.sensorframework.entities.TimeSpanSensor"
               acceptsMultiple="true"
               class="de.uka.ipd.sdq.codegen.rvisualisation.views.RDemandTimeReportView"
               displayName="R Total Demand Report"
               editorID="de.uka.ipd.sdq.codegen.rvisualization.views.DemandTimeReportView">
         </Visualisation>
      </extension>
      <extension
            point="org.eclipse.ui.editors">
         <editor
               class="de.uka.ipd.sdq.codegen.rvisualisation.views.RDemandTimeReportView"
               icon="icons/sample.gif"
               id="de.uka.ipd.sdq.codegen.rvisualization.views.DemandTimeReportView"
               name="R Total Demand Report">
         </editor>
      </extension>
            
      <extension
            point="de.uka.ipd.sdq.sensorframework.visualisation">
         <Visualisation
               acceptsData="de.uka.ipd.sdq.sensorframework.entities.TimeSpanSensor"
               acceptsMultiple="true"
               class="de.uka.ipd.sdq.codegen.rvisualisation.views.RDistributionComparisonView"
               displayName="R Distribution Comparison"
               editorID="de.uka.ipd.sdq.codegen.rvisualization.views.RDistributionComparison">
         </Visualisation>
      </extension>
      
      <extension
            point="org.eclipse.ui.editors">
         <editor
               class="de.uka.ipd.sdq.codegen.rvisualisation.views.RDistributionComparisonView"
               icon="icons/sample.gif"
               id="de.uka.ipd.sdq.codegen.rvisualization.views.RDistributionComparison"
               name="R Distribution Comparison">
         </editor>
      </extension>


</plugin>
