<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="de.uka.ipd.sdq.measurements.osscheduler.ui.OSSchedulerMeasurementsConfigurationDelegate"
            id="de.uka.ipd.sdq.measurements.osscheduler.ui.launchConfigurationType"
            modes="run, debug"
            name="OS Scheduler Measurements"
            public="true">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="de.uka.ipd.sdq.measurements.osscheduler.ui.OSSchedulerMeasurementsTabGroup"
            id="de.uka.ipd.sdq.measurements.osscheduler.ui.launchConfigurationTabGroup"
            type="de.uka.ipd.sdq.measurements.osscheduler.ui.launchConfigurationType">
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="de.uka.ipd.sdq.measurements.osscheduler.preferences"
            class="de.uka.ipd.sdq.measurements.osscheduler.ui.preferences.OSSchedulerSystemAdapterPreferencePage"
            id="de.uka.ipd.sdq.measurements.osscheduler.systemadapter.preferences"
            name="OS Scheduler SystemAdapter">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="de.uka.ipd.sdq.measurements.osscheduler.ui.preferences.MainPreferencePage"
            id="de.uka.ipd.sdq.measurements.osscheduler.preferences"
            name="OS Scheduler Measurements">
      </page>
   </extension>

</plugin>
