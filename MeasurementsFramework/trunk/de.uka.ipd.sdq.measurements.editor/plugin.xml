<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="measurements.presentation.MeasurementsModelWizardID"
            name="%_UI_MeasurementsModelWizard_label"
            class="measurements.presentation.MeasurementsModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/MeasurementsModelFile.gif">
         <description>%_UI_MeasurementsModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="measurements.presentation.MeasurementsEditorID"
            name="%_UI_MeasurementsEditor_label"
            icon="icons/full/obj16/MeasurementsModelFile.gif"
            extensions="measurements"
            class="measurements.presentation.MeasurementsEditor"
            contributorClass="measurements.presentation.MeasurementsActionBarContributor">
      </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="measurements.scheduler.presentation.SchedulerModelWizardID"
            name="%_UI_SchedulerModelWizard_label"
            class="measurements.scheduler.presentation.SchedulerModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SchedulerModelFile.gif">
         <description>%_UI_SchedulerModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="measurements.scheduler.presentation.SchedulerEditorID"
            name="%_UI_SchedulerEditor_label"
            icon="icons/full/obj16/SchedulerModelFile.gif"
            extensions="scheduler"
            class="measurements.scheduler.presentation.SchedulerEditor"
            contributorClass="measurements.scheduler.presentation.SchedulerActionBarContributor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category">
      </category>
      <wizard
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            class="measurements.presentation.MeasurementsModelWizard"
            icon="icons/full/obj16/MeasurementsModelFile.gif"
            id="measurements.presentation.TaskSetModelWizardID"
            name="%_UI_TaskSetModelWizard_label">
         <description>
            %_UI_TaskSetModelWizard_description
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="measurements.presentation.MeasurementsEditor"
            contributorClass="measurements.presentation.MeasurementsActionBarContributor"
            default="false"
            extensions="taskset"
            icon="icons/full/obj16/MeasurementsModelFile.gif"
            id="measurements.presentation.TaskSetEditorID"
            name="%_UI_TaskSetEditor_label">
      </editor>
   </extension>

</plugin>
