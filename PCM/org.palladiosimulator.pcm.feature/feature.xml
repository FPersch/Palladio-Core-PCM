         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.uka.ipd.sdq.pcm.editor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.pcm.resources"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
