      <import plugin="de.uka.ipd.sdq.units.edit" version="2.1.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.ecore.edit" version="2.9.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.uml2.common.edit" version="2.0.0" match="greaterOrEqual"/>
