      <import plugin="org.eclipse.core.databinding" version="1.4.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.databinding.property" version="1.4.200" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.mdsdprofiles.api" version="1.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="de.uka.ipd.sdq.pcm"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.pcm.resources"
