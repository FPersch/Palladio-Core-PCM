<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="de.uka.ipd.sdq.cip.launch.CIPLaunch"
            id="de.uka.ipd.sdq.cip.launchCIP"
            modes="run"
            name="PCM Completion Integration"
            public="true">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="de.uka.ipd.sdq.cip.launchCIP"
            icon="icons/completion.gif"
            id="de.uka.ipd.sdq.cip.CIPIcon">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="de.uka.ipd.sdq.cip.launch.CIPTabGroup"
            id="de.uka.ipd.sdq.cip.CIPTabGroup"
            type="de.uka.ipd.sdq.cip.launchCIP">
      </launchConfigurationTabGroup>
   </extension>

</plugin>
