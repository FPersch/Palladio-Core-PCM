<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="completionfeaturemodel"
    nsURI="http://sdq.ipd.kit.edu/CompletionFeatureModel/1.0" nsPrefix="completionfeaturemodel">
  <eClassifiers xsi:type="ecore:EEnum" name="FeatureState">
    <eLiterals name="NOT_SET"/>
    <eLiterals name="OPTIONAL" value="1"/>
    <eLiterals name="MANDATORY" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CompletionFeature" eSuperTypes="../../de.uka.ipd.sdq.featuremodel/model/featuremodel.ecore#//Feature">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="&lt;p>&#xD;&#xA;    decorator class of Feature&#xD;&#xA;&lt;/p>&#xD;&#xA;&lt;p>&#xD;&#xA;    Feature is extended by a QVT-Relation, a disambiguation rule which handles possible combinations of the Features of one&#xD;&#xA;    FeatureGroup, and a copy exception.&#xD;&#xA;&lt;/p>&#xD;&#xA;&lt;p>&#xD;&#xA;    The attribute FeatureState indicates, whether a CompletionFeature is a mandatory or a optional child of a Simple.&#xD;&#xA;&lt;/p>"/>
    </eAnnotations>
    <eOperations name="showFeatureState" ordered="false" lowerBound="1" eType="#//FeatureState">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="&lt;span&#xD;&#xA;style=&quot;WIDOWS: 2; TEXT-TRANSFORM: none; TEXT-INDENT: 0px; BORDER-COLLAPSE: separate; FONT: medium 'Times New Roman'; WHITE-SPACE: normal; ORPHANS: 2; LETTER-SPACING: normal; COLOR: rgb(0,0,0); WORD-SPACING: 0px; -webkit-border-horizontal-spacing: 0px; -webkit-border-vertical-spacing: 0px; -webkit-text-decorations-in-effect: none; -webkit-text-size-adjust: auto; -webkit-text-stroke-width: 0px&quot;&#xD;&#xA; class=&quot;Apple-style-span&quot;>&lt;span&#xD;&#xA;style=&quot;LINE-HEIGHT: 16px; FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif; FONT-SIZE: 11px&quot; class=&quot;Apple-style-span&quot;>The&#xD;&#xA;FeatureState of a CompletionFeature is MANDATORY, if the CompletionFeature is included in its&#xD;&#xA;simpleMandatory.mandatoryChildren. It is OPTIONAL, if it is included in its simpleOptional.optionalChildren. If neither of&#xD;&#xA;these conditions applies, the FeatureState is NOT_SET.&lt;/span>&lt;/span>&#xD;&#xA;if (simpleMandatory->size() = 1 and simpleMandatory.mandatoryChildren->size() > 0 and simpleMandatory.mandatoryChildren->includes(self)) then FeatureState::MANDATORY&#xD;&#xA; else (&#xD;&#xA; if (simpleOptional->size() = 1 and simpleOptional.optionalChildren->size() > 0 and simpleOptional.optionalChildren->includes(self)) then FeatureState::OPTIONAL&#xD;&#xA; else FeatureState::NOT_SET&#xD;&#xA; endif&#xD;&#xA; ) endif"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/uml2/1.1.0/GenModel">
        <details key="body" value="if (simpleMandatory->size() = 1 and simpleMandatory.mandatoryChildren->size() > 0 and simpleMandatory.mandatoryChildren->includes(self)) then FeatureState::MANDATORY&#xD;&#xA; else (&#xD;&#xA; if (simpleOptional->size() = 1 and simpleOptional.optionalChildren->size() > 0 and simpleOptional.optionalChildren->includes(self)) then FeatureState::OPTIONAL&#xD;&#xA; else FeatureState::NOT_SET&#xD;&#xA; endif&#xD;&#xA; ) endif"/>
      </eAnnotations>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="copyException" ordered="false"
        upperBound="-1" eType="ecore:EClass ../../de.uka.ipd.sdq.copyexception/model/CopyException.ecore#//Exception"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="disambiguationRule" ordered="false"
        upperBound="-1" eType="#//DisambiguationRule" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="transformationFragment"
        ordered="false" upperBound="-1" eType="ecore:EClass ../../edu.kit.ipd.sdq.qvtmodels/model/QVTRelation.ecore#//Relation"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="FeatureState" ordered="false"
        lowerBound="1" eType="#//FeatureState" changeable="false" volatile="true"
        transient="true" defaultValueLiteral="NOT_SET" derived="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="completionFeatureDiagram"
        ordered="false" eType="#//CompletionFeatureDiagram"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DisambiguationRule">
    <eStructuralFeatures xsi:type="ecore:EReference" name="feature" ordered="false"
        eType="#//CompletionFeature"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="disambiguationCondition"
        ordered="false" upperBound="-1" eType="ecore:EClass ../../edu.kit.ipd.sdq.qvtmodels/model/EssentialOCL.ecore#//OperationCallExp"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="transformationFragment"
        ordered="false" upperBound="-1" eType="ecore:EClass ../../edu.kit.ipd.sdq.qvtmodels/model/QVTRelation.ecore#//Relation"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CompletionFeatureDiagram" eSuperTypes="../../de.uka.ipd.sdq.featuremodel/model/featuremodel.ecore#//FeatureDiagram">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="decorator class of FeatureDiagram"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="referencedType" ordered="false"
        lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CompletionSimple" eSuperTypes="../../de.uka.ipd.sdq.featuremodel/model/featuremodel.ecore#//Simple">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="&lt;p>&#xD;&#xA;    decorator class of Simple&#xD;&#xA;&lt;/p>"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CompletionFeatureGroup" eSuperTypes="../../de.uka.ipd.sdq.featuremodel/model/featuremodel.ecore#//FeatureGroup">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="decorator class of FeatureGroup"/>
    </eAnnotations>
  </eClassifiers>
</ecore:EPackage>
