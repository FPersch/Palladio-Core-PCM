<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="sourcecodedecorator"
    nsURI="http://sdq.ipd.uka.de/PalladioComponentModel/SourceCodeDecorator/1.0" nsPrefix="sourcecodedecorator">
  <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
    <details key="documentation" value="Trace Link Model"/>
  </eAnnotations>
  <eSubpackages name="sourcecodedecorator" nsURI="http:///SourceCodeDecorator/sourcecodedecorator.ecore"
      nsPrefix="SourceCodeDecorator.sourcecodedecorator">
    <eClassifiers xsi:type="ecore:EClass" name="SourceCodeID" abstract="true" eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Unique position identifier for a source code artefact, class, package, method, branch, loop statement, or internal action."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="sourceCodeID" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="unique identifier for a certain position in code"/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="FileLevelSourceCodeLink" eSuperTypes="#//sourcecodedecorator/SourceCodeID">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Human readable information for the source code link"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="sourcefiles" ordered="false"
          upperBound="-1" eType="#//sourcecodedecorator/SourceFile"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SourceFile" eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="fullQualifiedName" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="for classes: full qualified class name; for C/C++ code: relative path to file + file name; if representing a method link do NOT add path information for inside file navigation."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Methods" eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="relativePathInFile" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="methodName" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="MethodLevelSourceCodeLink" eSuperTypes="#//sourcecodedecorator/FileLevelSourceCodeLink">
      <eStructuralFeatures xsi:type="ecore:EReference" name="methods" ordered="false"
          upperBound="-1" eType="#//sourcecodedecorator/Methods"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ControlFlowLevelSourceCodeLink" eSuperTypes="#//sourcecodedecorator/MethodLevelSourceCodeLink">
      <eStructuralFeatures xsi:type="ecore:EReference" name="controlflow" ordered="false"
          upperBound="-1" eType="#//sourcecodedecorator/ControlFlow"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ControlFlow" eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="lineNumber" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="starting line"/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SourceCodeDecoratorRepository" eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier">
      <eStructuralFeatures xsi:type="ecore:EReference" name="filelevelsourcecodelink"
          ordered="false" upperBound="-1" eType="#//sourcecodedecorator/FileLevelSourceCodeLink"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="methodlevelsourcecodelink"
          ordered="false" upperBound="-1" eType="#//sourcecodedecorator/MethodLevelSourceCodeLink"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="controlflowlevelsourcecodelink"
          ordered="false" upperBound="-1" eType="#//sourcecodedecorator/ControlFlowLevelSourceCodeLink"
          containment="true"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="PCMLink" nsURI="http:///SourceCodeDecorator/PCMLink.ecore" nsPrefix="SourceCodeDecorator.PCMLink">
    <eClassifiers xsi:type="ecore:EClass" name="ComponentFileLink" eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier">
      <eStructuralFeatures xsi:type="ecore:EReference" name="filelevelsourcecodelink"
          ordered="false" lowerBound="1" eType="#//sourcecodedecorator/FileLevelSourceCodeLink"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="repositorycomponent"
          ordered="false" lowerBound="1" eType="ecore:EClass ../../de.uka.ipd.sdq.pcm/model/pcm.ecore#//repository/RepositoryComponent"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SEFFMethodLink" eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier">
      <eStructuralFeatures xsi:type="ecore:EReference" name="methodlevelsourcecodelink"
          ordered="false" lowerBound="1" eType="#//sourcecodedecorator/MethodLevelSourceCodeLink"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="serviceeffectspecification"
          ordered="false" lowerBound="1" eType="ecore:EClass ../../de.uka.ipd.sdq.pcm/model/pcm.ecore#//seff/ServiceEffectSpecification"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ActionControlFlowLink" eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier">
      <eStructuralFeatures xsi:type="ecore:EReference" name="controlflowlevelsourcecodelink"
          ordered="false" lowerBound="1" eType="#//sourcecodedecorator/ControlFlowLevelSourceCodeLink"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="abstractaction" ordered="false"
          lowerBound="1" eType="ecore:EClass ../../de.uka.ipd.sdq.pcm/model/pcm.ecore#//seff/AbstractAction"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PCMLinkRepository" eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier">
      <eStructuralFeatures xsi:type="ecore:EReference" name="componentfilelink" ordered="false"
          upperBound="-1" eType="#//PCMLink/ComponentFileLink" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="seffmethodlink" ordered="false"
          upperBound="-1" eType="#//PCMLink/SEFFMethodLink" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="actioncontrolflowlink"
          ordered="false" upperBound="-1" eType="#//PCMLink/ActionControlFlowLink"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="loggingPositionIds" ordered="false"
          upperBound="-1" eType="#//loggerlink/LoggingPositionId" containment="true"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="loggerlink" nsURI="http:///SourceCodeDecorator/loggerLink.ecore"
      nsPrefix="SourceCodeDecorator.loggerLink">
    <eClassifiers xsi:type="ecore:EClass" name="LoggingPositionId">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="loggingPositionId" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="serviceEffectSpecification"
          ordered="false" lowerBound="1" eType="ecore:EClass ../../de.uka.ipd.sdq.pcm/model/pcm.ecore#//seff/ServiceEffectSpecification"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="abstractAction" ordered="false"
          lowerBound="1" eType="ecore:EClass ../../de.uka.ipd.sdq.pcm/model/pcm.ecore#//seff/AbstractAction"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
