       uri = "http://sdq.ipd.uka.de/IdentifierPackage" 
       class = "IdentifierPackage.provider.IdentifierPackageItemProviderAdapterFactory"
       supportedTypes = 
         "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
