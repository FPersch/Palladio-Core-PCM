/*
 * ReverseICT with cloning of model.
 */

/* Access? */
import ReverseICT;

modeltype PCM uses "http://sdq.ipd.uka.de/PalladioComponentModel/4.0";
modeltype PCM_REP uses "http://sdq.ipd.uka.de/PalladioComponentModel/Repository/4.0";

transformation ReverseICTClone(in input : PCM, out output : PCM);

main() {
	input.rootObjects()->deepclone();
	var transform : Transformation := new ReverseICT(output);
	transform.transform();
}
