<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="edu.kit.ipd.sdq.simcomp.middleware.simulationComponentType">
      <simulation_component_type
            id="edu.kit.ipd.sdq.simcomp.resource.active"
            name="Active Resource Simulation Component"
            type_interface="edu.kit.ipd.sdq.simcomp.resource.active.component.IActiveResource">
         <simulation_context_field
               id="edu.kit.ipd.sdq.simcomp.resource.active.ctx.container"
               name="Container"
               value_provider="edu.kit.ipd.sdq.simcomp.resource.active.context.ContainerContextValueProvider">
         </simulation_context_field>
         <simulation_context_field
               id="edu.kit.ipd.sdq.simcomp.resource.active.ctx.resource_type"
               name="Resource Type"
               value_provider="edu.kit.ipd.sdq.simcomp.resource.active.context.ResourceTypeContextValueProvider">
         </simulation_context_field>
      </simulation_component_type>
   </extension>

</plugin>
